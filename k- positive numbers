function positiveSum(arr) {
let sum = 0;

for (let i = 0; i < arr.length; i++) {
  if(arr[i] === 0) {
  return null
  }
    if(arr[i] > 0) {
     sum += arr[i]
      }
  console.log(arr[i])
  }
   
  return sum
}

// loop over the array
// check if the numbers are even
// if number is even, add to the total sum

// loop over the array
// check if the numbers are even
// if number is even, add to the total sum
// return sum

positiveSum([1,2,3,4,5])
