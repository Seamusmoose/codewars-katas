
function divisors(integer) {
  let array = [];

  for (let i = 2; i < integer -1; i++) {
       if (integer % i === 0) array.push(i)           
  }
  if (array.length === 0) return '' + integer +  ' is prime'
  return array
 
};



divisors(12)
//[3, 5]);
divisors(25) 
 // [2, 3, 4, 6]);
divisors(13) 
//"13 is prime");