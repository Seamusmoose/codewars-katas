function arrayMadness(a, b) {
  let res1 = 0;
  let res2 = 0;

  a.map(el => {
  res1 += el ** 2
  })
  
  b.map(el => {
  res2 += el ** 3
  })


return res1 > res2 ? true : false
 
}

// calc each array and compare


arrayMadness([4,5,6],[1,2,3])
//true);

arrayMadness([5,6,7],[4,5,6])
  //, false);